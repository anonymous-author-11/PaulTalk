<?xml version="1.0" ?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface" uri="http://mycompany.example/myapp"> <!-- App's unique URI -->
  <name>DoCompile</name>
  <summary>Compiles generic.mini into the directory</summary>

  <implementation id="." arch="*-src" version="1.0.1">
    <command name='compile' path='c:/windows/py.exe'>
      <arg>c:/users/paulk/onedrive/documents/pl/pypl/compiler.py</arg>
      <arg>c:/users/paulk/onedrive/documents/pl/pypl/lib/generic.mini</arg>
      <arg>-o</arg>
      <arg>$DISTDIR/compiled.exe</arg>
      <arg>--build-dir</arg>
      <arg>$BUILDDIR</arg>
    </command>
  </implementation>
</interface>